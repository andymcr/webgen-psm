[comment encoding = UTF-8 /]
[module htaccess('http://www.cs.man.ac.uk/mdsd/2010/Jsf')]


[template public generateHtaccess(jsfModel : JsfModel)]
[file ('x.htaccess', false)]
<IfModule mod_rewrite.c>
    RewriteEngine on
    RewriteBase /[jsfModel.baseURL/]/
    RewriteCond $1 !^(css|index\.php|images|robots\.txt)
    RewriteRule ^(.*)$ /index.php/$1 ['['/]L[']'/]
</IfModule>
[/file]
[/template]
