            id="work.andycarpenter.metamodel.orm.presentation.OrmEditorID"
            name="%_UI_OrmEditor_label"
            icon="icons/full/obj16/OrmModelFile.gif"
            extensions="orm"
            class="work.andycarpenter.metamodel.orm.presentation.OrmEditor"
            contributorClass="work.andycarpenter.metamodel.orm.presentation.OrmActionBarContributor">
      </editor>
