      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="work.andycarpenter.metamodel.orm.presentation.OrmModelWizardID"
            name="%_UI_OrmModelWizard_label"
            class="work.andycarpenter.metamodel.orm.presentation.OrmModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/OrmModelFile.gif">
